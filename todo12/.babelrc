{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "absolateRuntime": false,
        "corejs": 2,
        "regenerator": true,
        "useESModule": false
      }
    ]
  ]
}